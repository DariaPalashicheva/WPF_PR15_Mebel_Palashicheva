<Window x:Class="WPF_PR15_Mebel_Palashicheva.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_PR15_Mebel_Palashicheva"
        mc:Ignorable="d"
        Title="Мебель"  Height="650" Width="1200" MinHeight="650" MinWidth="1200" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <Rectangle Fill="DarkCyan"/>
        <Image Source="Resources/iconn.png" HorizontalAlignment="Left" Height="70" Width="70" Margin="20,0,0,0"/>
        <TextBlock Text="Аксессуары для сна" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" FontSize="40"/>
        <Frame Grid.Row="1" x:Name="frmMain" NavigationUIVisibility="Hidden"/>
        <Rectangle Fill="DarkCyan" Grid.Row="2"/>
    </Grid>
</Window>
